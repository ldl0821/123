extends ../../../../../views/layout
append headjavascript
   script(type='text/javascript',src='../Statement/web/controller/bootstrap-select.js')
   script(type='text/javascript',src='./ProductUpDown/web/controller/partsummaryController.js')
   link(type='text/css',href='../Statement/web/css/bootstrap-select.css',rel='stylesheet') 
   link(type='text/css',href='./ProductUpDown/web/css/partsummaryController.css',rel='stylesheet')
block content
  .row-fluid
    .span12
      h3.page-title(data-lang='AccountManage')
        |零件查询
      ul.breadcrumb
        li
          i.icon-home
          a(href='/') 主页
          i.icon-angle-right
        li
          a(href='javascript:;', data-lang='System') 产品上下线
          i.icon-angle-right
        li
          a(href='javascript:;', data-lang='AccountManage') 零件查询
  .row-fluid
     table(width='100%', cellpadding='0', cellspacing='0')
      tbody
        tr
          td(valign='top', style='padding-left:5px;')
            .control-group(style='height:30px')
              .span(style='width: 315px !important;')
                  .control-group(style='margin-bottom: 5px;')
                      label.control-label(data-lang='Common.PASSWORD',style='margin-right:10px') #{lang.Alarm.MachineGroup}
                      input(type="text" id="MAC_NBRname"  name="MAC_NBRname" class="span6 m-wrap")
              .span(style='width: 350px !important;')
                  .control-group(style='margin-bottom: 5px;position: relative;top:-5px')
                      label.control-label(data-lang='Common.PASSWORD',style='margin-right:10px;margin-top:6px;') #{lang.Alarm.MachineName}
                      select(multiple  id="my-select" data-live-search-placeholder="搜索"  title=""  data-actions-box="true" class='selectpicker')
              .span(style='width: 314px !important;margin-left:0px !important;')
                  .control-group(style='margin-bottom: 5px;')
                      label.control-label(data-lang='Common.PASSWORD',style='margin-right:10px') #{lang.Alarm.MachineNo}
                      input( id="my-selects"  class='selectpicker')
            .control-group(style='margin-top:24px')
              label.control-label(data-lang='Common.END_TIME') 
                span 零件号
                input#filter.m-wrap(type='text', placeholder='', common.user_name")"='')
              label.control-label.clear-fix(data-lang='Common.END_TIME') 
                span 开始时间
                input#startTime.m-wrap(type='text', name='startTime')
              label.control-label.clear-fix(data-lang='Common.END_TIME') 
                span 结束时间
                input#endTime.m-wrap(type='text', name='endTime')
              button#search.btn.green(title='', switch")"='')
                  | 查询
                  i.icon-search
     #grid
        script#treeview-template(type='text/kendo-ui-template').
            <i class="#:item.icon#"></i>
            <span nodeid="#:item.id#" pid="#:item.PID#"  onclick='addOnClick(this)' level_nbr="#:item.LEVEL_NBR#" attr="treenode" flag="#:item.flag#">#:item.text#</span>


